#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {

    int w, x, y;

    scanf("%d %d %d", &w, &x, &y);

    int fixed_cost = 100;

    int profit = (w * x) - (w * y) - fixed_cost;

    printf("%d\n", profit);

    return 0;
}
