#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, value;
    double sum = 0.0;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &value);
        sum += value;
    }

    double mean = sum / n;
    printf("Array Mean Value is %.2f\n", mean);

    return 0;
}

