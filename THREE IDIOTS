#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float x1, y1, x2, y2, midX, midY;

    scanf("%f %f %f %f", &x1, &y1, &x2, &y2);

    midX = (x1 + x2) / 2;
    midY = (y1 + y2) / 2;

    printf("Binoy's house is located at (%.1f,%.1f)\n", midX, midY);

    return 0;
}
