#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, value;
    double product = 1.0, sum = 0.0;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &value);
        product *= value;
        sum += value;
    }

    double mean = sum / n;

    printf("Product of the array:%.2f\n", product);
    printf("Mean of the array:%.3f\n", mean);

    return 0;
}

