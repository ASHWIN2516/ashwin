#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
        int number;

    scanf("%d", &number);
 
    if (number > 0) {
        printf("The Given Number %d is Positive.\n", number);
    } else if (number < 0) {
        printf("The Given Number %d is Negative.\n", number);
    } else {
        printf("The Given Number %d is Zero.\n", number);
    }
    
    return 0;
}
