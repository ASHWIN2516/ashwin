#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char ch;

    scanf(" %c", &ch);

    //ch = toupper(ch);

    if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'||ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
        printf("The Character %c is Vowel\n", ch);
    } else if (isalpha(ch)) {
        printf("The Character %c is Consonant\n", ch);
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
