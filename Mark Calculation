#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char name[50];
    int mark1, mark2, mark3, mark4, mark5;

    scanf("%s %d %d %d %d %d", name, &mark1, &mark2, &mark3, &mark4, &mark5);

    int total_marks = mark1 + mark2 + mark3 + mark4 + mark5;
    float average_marks = total_marks / 5.0;

    printf("Name of the Student:%s\n", name);
    printf("Total marks:%d\n", total_marks);
    printf("Average marks:%.1f\n", average_marks);

    return 0;
}
