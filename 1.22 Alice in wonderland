#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int number, sum_of_digits;

    scanf("%d", &number);

    sum_of_digits = (number / 10) + (number % 10);

    printf("Bird said:%d\n", number);
    printf("Alice must go in path-%d\n", sum_of_digits);

    return 0;
}
