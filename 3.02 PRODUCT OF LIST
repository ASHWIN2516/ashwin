#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n); 

    int value;
    double product = 1.0;

    for (int i = 0; i < n; i++) {
        scanf("%d", &value);
        product *= value;
    }

    printf("The Product of list value is %.2f\n", product);
    return 0;
}
